Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519

Host ec2
  Hostname localhost
  User ubuntu
  Port 2223
  IdentityFile ~/.ssh/jamie-mcmillanBox.pem
  IdentitiesOnly yes

